@model CrudCT.Application.DTOs.TipoTelefoneDTO

<h2>Adicionar Tipo Telefone</h2>

<form asp-action="Create" method="post">

    <div class="form-horizontal">

        <hr />

        <div asp-validation-summary="All" class="text-danger"></div>

        <div class="form-group">
            <label asp-for="DescricaoTipoTelefone" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="DescricaoTipoTelefone" class="form-control" />
                <span asp-validation-for="DescricaoTipoTelefone" class="text-danger"></span>
            </div>
        </div>

    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-success" />
            <a asp-action="Index" class="btn btn-info">Voltar</a>
        </div>
    </div>
</form>
